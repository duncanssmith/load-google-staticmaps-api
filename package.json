{
  "name": "load-google-static-maps-api",
  "version": "1.3.3",
  "description": "A thin, Promise-returning helper for loading the Google Static Maps JavaScript API.",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/load-google-maps-api.git"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "gzip-size-cli": "^3.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier-standard": "^9.1.1",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "tape": "^4.10.1",
    "tape-run": "^5.0.0",
    "uglify-js": "^3.5.3"
  },
  "scripts": {
    "clean": "rimraf '*.log'",
    "fix": "prettier-standard '*.js'",
    "lint": "standard '*.js'",
    "test": "browserify test.js | tape-run",
    "weight": "uglifyjs index.js --compress --mangle --toplevel | gzip-size"
  },
  "lint-staged": {
    "*.js": [
      "standard",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "api",
    "google",
    "static",
    "google-static-maps",
    "google-static-maps-api",
    "loader",
    "maps",
    "promise"
  ]
}
